#include <iostream>
using namespace std;

int main() {
    int n, digit, smallest = 9;
    cout << "Enter number: ";
    cin >> num;

    while(n> 0) {
        digit = n% 10;
        if(digit < smallest)
            smallest = digit;
        n/= 10;
    }

    cout << "Smallest digit = " << smallest;
    return 0;
}
