#include <iostream>
using namespace std;

int main() {
    string correctpassword = "khulja";
    string password;
    while(true){
    cout<<"enter password :";
    cin>>password;
    if(password == correctpassword) {
            cout << "====WELCOME====";
            break;
        }
        else {
            cout << "Wrong Password\n";
        }
    }
return 0;
}
