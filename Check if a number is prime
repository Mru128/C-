#include<iostream>
using namespace std;
int main(){
    int i,n,count;
    cout<<"enter number";
    cin>>n;
    count=0;
    for(i=1;i<=n;i++)
    { 
        if(n%i==0)
        count++;
    }
        if(count==2)
        cout<<"number is prime";
        else
        cout<<"number is composite";
        return 0;
    }
